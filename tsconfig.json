{
  "name": "prime-video-arrival-checker",
  "version": "0.0.1",
  "author": "resessh",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": false,
    "preserveConstEnums": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "target": "es2015",
    "experimentalDecorators": true,
    "sourceMap": false
  },
  "license": "MIT",
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "./node_modules"
  ]
}
